{
  "name": "shoppingcart-react-express-sqlite-app",
  "version": "1.0.0",
  "description": "App for adding products to shopping cart and checkout built with React, Express and SQLite.",
  "repository": {
    "type": "git"
  },
  "private": false,
  "license": "GNU",
  "keywords": [
    "app",
    "eCom"
    "products",
    "express",
    "javascript",
    "react",
    "sqlite",
    "typescript"
  ],
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.1.0"
  },
  "main": "src/index.tsx",
  "prox": "http://localhost:4001",
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "body-parser": "^1.20.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "^4.18.2",
    "helmet": "3.22.0",
    "knex": "^2.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/validator": "^13.1.3",
    "concurrently": "5.1.0",
    "nodemon": "^2.0.20",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.0.2",
    "typescript": "3.8.3"
  }
}
